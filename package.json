{
    "name": "condition",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "start": "esno ./start.ts",
        "v2": "esno ./start.ts version=2.6.0",
        "v2.7": "esno ./start.ts version=2.7.13",
        "v3": "esno ./start.ts version=3.2.40",
        "switch2": "pnpm --filter \"@xiaohaih\\core\" vue2 && pnpm --filter \"@xiaohaih\\condition-el\" vue2",
        "switch2.7": "pnpm --filter \"@xiaohaih\\core\" vue2.7 && pnpm --filter \"@xiaohaih\\condition-el\" vue2.7",
        "switch3": "pnpm --filter \"@xiaohaih\\core\" vue3 && pnpm --filter \"@xiaohaih\\condition-el-plus\" vue3",
        "build:v2": "pnpm build:core && pnpm build:el",
        "build:v2.7": "pnpm build:core && pnpm build:el",
        "build:v3": "pnpm build:core && pnpm build:plus",
        "----------------------------": "----------------------------",
        "---------------------------|": "----------------------------",
        "publishing": "pnpm publish:core && pnpm publish:el && pnpm publish:plus",
        "publish:core": "pnpm --filter @xiaohaih/condition-core publish --access=public",
        "publish:el": "pnpm --filter @xiaohaih/condition-el publish --access=public",
        "publish:plus": "pnpm --filter @xiaohaih/condition-el-plus publish --access=public",
        "|---------------------------": "----------------------------",
        "build": "pnpm build:core && pnpm build:el && pnpm build:plus",
        "build:core": "pnpm --filter @xiaohaih/condition-core build",
        "build:el": "pnpm --filter @xiaohaih/condition-el build",
        "build:plus": "pnpm --filter @xiaohaih/condition-el-plus build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "xiaohaih",
    "license": "ISC",
    "devDependencies": {
        "cac": "^6.7.14",
        "esno": "^0.16.3",
        "rimraf": "^5.0.1",
        "typescript": "^5.0.2",
        "vite": "4.3.2",
        "vue-tsc": "^1.4.2"
    },
    "pnpm": {
        "overrides": {
            "vite": "4.3.2",
            "vue": "2.6.0",
            "vue-template-compiler": "2.6.0"
        },
        "patchedDependencies": {
            "vite@4.3.2": "patches/vite@4.3.2.patch"
        }
    }
}